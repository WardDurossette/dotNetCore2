@model List<WWWROOT.Models.Book>

<h3>Books - Index</h3>
<div class="container">
  <div class="row">
    <div class="col-md-3">
      <hr />
      <ul>
      @foreach(var item in Model)
      {
        <li>@Html.DisplayFor(ModelItem => item.BookTitle)</li>
      }
      </ul>
      <hr />
    </div>
  </div>
</div>
